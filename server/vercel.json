{
    "rewrites": [
        {
            "source": "/((?!api\\/.*).*)",
            "destination": "/index.html"
        }
    ],
    "version": 2,
    "name": "Blinkit",
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}